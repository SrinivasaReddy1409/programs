#palindrome
a=int(input("enter number:");
rev=0;
while num!=0:
    rem=a%10;
    rev=rev*10+rem;
    num=num//10;
if (rev==num):
    print("Palindrome");
else :
    print("Not Palindrome");

#armstrong
a=int(input("enter 3 digit number:");
temp=a;
sum=0;
while num!=0:
    rem=a%10;
    sum=sum+rem**3;
    num=num//10;
if(sum==temp):
        print("Armstrong number");
else :
        print("Not Armstrong number");
        
#area of triangle
h=int(input("Enter height of triangle:");
b=int(input("Enter base of triangle:");
print("Area of triangle : %d"%(((b*h)/2));

#area of square
s=int(input("Enter the side of square:");
print("Area of square: %d"%(s**2));

#area of rectangle
l=int(input("Enter the length of rectangle:");
b=int(input("Enter the breadth of reactangle:");
print("Area of rectangle: %d"%(l*b));

