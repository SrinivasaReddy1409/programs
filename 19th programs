#fibonacci series upto n numbers
n=int(input("enter no. of numbers in series:");
a=0;
b=1;
print(a,end=" ");
print(b,end=" ");
for i in range(n-2):
    c=a+b;
    print(c,end=" ");
    a=b;
    b=c;


        
                
        
    
